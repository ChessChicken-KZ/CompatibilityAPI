{
  "schemaVersion": 1,
  "id": "compatibility_clapi",
  "version": "${version}",

  "name": "CompatibilityAPI (CursedLegacyAPI)",
  "description": "CursedLegacyAPI support module for CompatibilityAPI",
  "authors": [
    "ChessChicken-KZ"
  ],
  "contact": {
    "homepage": "https://github.com/ChessChicken-KZ/OldFabricCompatibility",
    "sources": "https://github.com/ChessChicken-KZ/OldFabricCompatibility",
    "issues": "https://github.com/ChessChicken-KZ/OldFabricCompatibility/issues"
  },

  "license": "MIT",
  "icon": "assets/compatibility_clapi/icon.png",

  "environment": "*",
  "entrypoints": {
    "init": [
      "kz.chesschicken.compatibility.clapi.EventCall"
    ],
    "preLaunch": [
      "kz.chesschicken.compatibility.clapi.APICall"
    ]
  },
  "mixins": [
    "compatibility_cla.mixins.json"
  ],

  "depends": {
    "minecraft": "1.0.0-beta.7.3",
    "compatibility": "*"
  },

  "custom": {
    "modmenu:api": true
  }
}
